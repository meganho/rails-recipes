<h1>Email</h1>
<th>Groups</th>

<table class="table">
<tr>
  <th>ID</th>
  <th>Email</th>
  <th>Actions</th>
</tr>

      <% @users.each do |user| %>
  <tr>

    <td><%= user.id %></td>
    <td><%= user.email %></td>
    <td>
       <% user.groups.each do |g| %>
         <%= g.name %><br>
       <% end %>
     </td>
  <td><%= link_to "Edit Profile", edit_admin_user_path: [:id] , :class => "btn btn-default" %></td>
  </tr>
<% end %>
</table>
